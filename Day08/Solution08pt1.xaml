    <ui:MultipleAssign sap2010:Annotation.AnnotationText="Convert to array of Int.&#xD;&#xA;Calculate the number of pixels per layer.&#xD;&#xA;Split the Array into the layers (2D array).&#xD;&#xA;Find the array with the least zeros.&#xD;&#xA;Calculate the checksum." DisplayName="Calc checksum" sap:VirtualizedContainerService.HintSize="338,309" sap2010:WorkflowViewState.IdRef="MultipleAssign_1">
      <ui:MultipleAssign.AssignOperations>
        <scg:List x:TypeArguments="ui:AssignOperation" Capacity="8">
